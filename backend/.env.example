# Database Configuration
POSTGRES_USER=tu_usuario_bd
POSTGRES_PASSWORD=tu_contraseña_bd
POSTGRES_DB=tu_nombre_bd
DB_HOST=postgres
DB_PORT=5432

# JWT
JWT_SECRET_KEY=your-secret-key-change-in-production
JWT_ALGORITHM=HS256
JWT_EXPIRATION_HOURS=24

# CORS
CORS_ORIGINS=["http://localhost:3000","http://localhost:5173"]

# Application
APP_NAME=TaskFlow
APP_VERSION=1.0.0
DEBUG=True

# Server Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Admin User - Configuración del usuario administrador inicial
# Se crea automáticamente al iniciar el servidor si no existe
# Define tus propias credenciales aquí
ADMIN_USERNAME=tu_admin_username
ADMIN_EMAIL=tu_admin@example.com
ADMIN_PASSWORD=tu_contraseña_segura_aqui

